@page
@model Proyecto.Pages_Technicians.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Technician[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Technician[0].BirthDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Technician) {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BirthDate)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.TechnicianID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.TechnicianID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.TechnicianID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
